{
    "name": "hurongsheng/laravel-route-doc",
    "type": "library",
    "description": "easily create doc based on route.php",
    "version": "2.0.1",
    "keywords": [
        "laravel",
        "route",
        "api",
        "doc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hurongsheng",
            "email": "plokm031004@126.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "laravel/framework": "5.*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
        "classmap": [
            "Controllers",
            "Lib",
            "Models"
        ],
        "files": [
            "RouteDoc.php",
            "RouteDocServiceProvider.php",
            "RouteTransClass.php"
        ]
    }
}
